<!DOCTYPE html>
<html>
<head>
  <title>TabataRailsApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href='https://fonts.googleapis.com/css?family=Codystar|Josefin+Sans' rel='stylesheet' type='text/css'>
</head>
<body>
  <div id='wrap'>
    <div class="flash">
      <% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>"><%= value %></div>
      <% end %>
    </div>
    <%= render 'nav_bar' %>
    <%= yield %>
  </div>
</body>
</html>
