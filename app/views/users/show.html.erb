<h1> -- <%= @user.username %> -- </h1>  
  
<%= render 'divider' %> 

<h2>Saved Workouts</h2>
<% @user.workouts.each do |w| %>
<div> <%= link_to "#{w.name}", workout_path(w) %> / <%= link_to 'edit', edit_workout_path(w) %> </div>
<% end %>
 
 <br>
<%= render 'divider' %>

<h2> Created Exercises </h2>
<% @user.exercises.each do |ex| %>
  <div id="exercise_list"><%= link_to "#{ex.name}", exercise_path(ex) %> / <%= link_to 'edit', edit_exercise_path(ex) %></div>
<% end %>


